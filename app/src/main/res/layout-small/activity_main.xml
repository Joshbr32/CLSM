<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.gridlayout.widget.GridLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="350dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="90dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <Button
            android:id="@+id/timesheet_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="timesheet"
            android:text="@string/timesheet_button_text"
            app:layout_column="0"
            app:layout_row="0" />

        <Button
            android:id="@+id/workorder_button"
            android:layout_width="153dp"
            android:layout_height="wrap_content"
            android:onClick="workOrder"
            android:text="@string/workorder_button_text"
            app:layout_column="2"
            app:layout_row="0" />

        <Button
            android:id="@+id/inventory_button"
            android:layout_width="132dp"
            android:layout_height="50dp"
            android:onClick="inventory"
            android:text="@string/inventory_button_text"
            app:layout_column="0"
            app:layout_row="1" />

        <Button
            android:id="@+id/form_storage_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="formStorage"
            android:text="@string/formstorage_button_text"
            app:layout_column="2"
            app:layout_row="1"
            tools:layout_editor_absoluteX="63dp"
            tools:layout_editor_absoluteY="153dp" />


    </androidx.gridlayout.widget.GridLayout>

    <ImageView
        android:id="@+id/colonial_logo_main_menu"
        android:layout_width="385dp"
        android:layout_height="192dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="44dp"
        android:contentDescription="TODO"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/colonial" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="443dp"
        android:layout_height="52dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:layout_editor_absoluteX="-20dp"
        tools:layout_editor_absoluteY="641dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/icons8_home_24"
        tools:layout_editor_absoluteX="179dp"
        tools:layout_editor_absoluteY="641dp" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="39dp"
        android:layout_height="28dp"
        app:srcCompat="@drawable/icons8_filing_cabinet_50"
        tools:layout_editor_absoluteX="251dp"
        tools:layout_editor_absoluteY="645dp" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/icons8_time_24"
        tools:layout_editor_absoluteX="312dp"
        tools:layout_editor_absoluteY="645dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/icons8_clipboard_24"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="640dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/icons8_question_mark_24"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="640dp" />


</androidx.constraintlayout.widget.ConstraintLayout>